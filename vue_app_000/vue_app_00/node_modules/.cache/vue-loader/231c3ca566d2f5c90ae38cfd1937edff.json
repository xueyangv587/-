ui-pull-left\" :src=\"item.img_url\" alt=\"\">\r\n          <div class=\"mui-media-body\">\r\n            {{item.title}}\r\n            <p class=\"mui-ellipsis p_display\">\r\n              <span>\r\n                {{item.ctime | datatimeFilter}}\r\n              </span>\r\n              <span>点击{{item.point}}次</span>\r\n            </p>\r\n          </div>\r\n        </router-link>\r\n      </li>\r\n    </ul>\r\n    <mt-button type=\"primary\" size=\"large\" @click=\"getMore()\">加载更多</mt-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return{\r\n      list:[],\r\n      pno:1,\r\n      pageSize:7\r\n    }\r\n  },\r\n  methods:{\r\n    ajax(){\r\n      var url=\"http://127.0.0.1:3000/newslist\"\r\n      this.axios.get(url).then(result=>{\r\n        this.list=result.data.data;\r\n      })\r\n    },\r\n    //加载更多\r\n    getMore(){\r\n      this.pno++;\r\n      //发送请求get\r\n      var url=\"http://127.0.0.1:3000\";\r\n      url+=\"/newslist?pno=\"+this.pno;\r\n      url+=\"&pageSize=\"+this.pageSize;\r\n      this.axios.get(url).then(result=>{\r\n           //concat  拼接数据 把所有请求次数的数据整合一起\r\n       var rows=this.list.concat(result.data.data);\r\n       console.log(result.data.data);\r\n       this.list=rows;\r\n      })\r\n    }\r\n  },\r\n  created(){\r\n    this.ajax();\r\n    console.log(this.$route.query.nid);\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n/* 日期和点击次数两端对齐 */\r\n  .p_display{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    color:#226aff;\r\n  }\r\n</style>"]}]}